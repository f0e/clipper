	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "debug clipper",
			"runtimeExecutable": "yarn",
			"runtimeArgs": ["run", "dev"],
			"cwd": "${workspaceFolder}/clipper",
			"skipFiles": [
				"${workspaceFolder}/node_modules/**/*.js",
				"<node_internals>/**/*.js",
				"**/*async_hook*.js"
			]
		}
	]
}
